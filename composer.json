    "type": "library",
    "require": {
        "php": "^7.3.0",
        "ext-bcmath": "*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "symfony/http-client": "^5.4",
        "guzzlehttp/guzzle": "^7.9"
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Suporteleiloes\\WebsiteApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tiago Felipe",
            "email": "tiago@tiagofelipe.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
